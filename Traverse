#include<stdio.h>
void main()
{
    int a[10],i,n;
    printf("Enter the length of a array:");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        printf("Enter the element in array:");
        scanf("%d",&a[i]);
    }
    
    for(i=0;i<n;i++)
    {
        printf("%d",a[i]);
        printf(" ");
    }
}
