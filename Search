#include<stdio.h>
void main()
{
    int a[10],i,n,x;
    printf("Enter the length of a array:");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        printf("Enter the element in array:");
        scanf("%d",&a[i]);
    }
    
    printf("Enter the element in array you want to find:");
    scanf("%d",&x);
    
    for(i=0;i<n;i++)
    {
     if(a[i]==x)
     {
        printf("Element found");
        break;
     }
    }
}
